<div id="<%= dom_id user %>">

  <%= image_tag user.image ,style: "max-width: 250px;"%> 
  <p>
    <strong style="color : Red">Name:</strong>
    <%= user.name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Password:</strong>
    <%= user.password %>
  </p>

</div>


